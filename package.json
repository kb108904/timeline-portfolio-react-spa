{
  "name": "projecttimeline",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vitest --exclude templates --run && tsc -b && cross-env VITE_AWS_ACCESS_KEY_ID=$VITE_AWS_ACCESS_KEY_ID VITE_AWS_SECRET_ACCESS_KEY=$VITE_AWS_SECRET_ACCESS_KEY VITE_AWS_REGION=$VITE_AWS_REGION VITE_S3_BUCKET_NAME=$VITE_S3_BUCKET_NAME VITE_IS_UNDER_CONSTRUCTION=$VITE_IS_UNDER_CONSTRUCTION vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --exclude templates",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "aws-sdk": "^2.1674.0",
    "bootstrap": "^5.3.3",
